#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int number,digitlast,digit;
    scanf("%d",&number);
    if(number>=0 && number<=9){
    digitlast=digit=number;
}
    else{
    digitlast=number%10;
    number=number/10;
    while(number!=0){
     digit = number;
     number=number/10;
     
    }}
    printf("%d %d",digit,digitlast);
    return 0;
}
