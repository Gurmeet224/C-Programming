#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x,y,z;
    scanf("%d %d %d",&x,&y,&z);
    if(y>=x && y>=z)
        printf("%d",y);
    else if(x>=y && x>=z)
        printf("%d",x);
    else if(z>=x && z>=y)
        printf("%d",z);   
    return 0;
}
