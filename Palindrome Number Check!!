#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int num,rem,dig=0,temp;
    scanf("%d",&num);
    temp=num;
    if(num<0){
        num = -num;
    }
    while(num!=0){
        rem = num%10;
        num=num/10;
        dig=dig*10+rem;
    }
    if(temp==dig){
        printf("Palindrome");
        
    }
    else{
        printf("Not Palindrome");
    }
    return 0;
}
